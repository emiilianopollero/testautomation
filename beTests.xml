<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Api BE tests" verbose="1" >
    <listeners>
        <listener class-name="SeleniumAutomation.Utils.TestReporter"/>
    </listeners>
    <test name="BE API tests regression">
        <classes>
